{
  "name": "postcss-plugin-d-ts",
  "version": "0.1.0",
  "description": "PostCSS plugin generates `.d.ts` for all processed style sheets",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "d.ts",
    "typescript",
    "declare",
    "declaration",
    "sass"
  ],
  "scripts": {
    "test": "npm run jest && npm run spec/tsc",
    "jest": "jest --config jest.config.json",
    "prejest": "jest --clearCache && find . -type f -iname '*.css.d.ts' -exec rm {} \\;",
    "spec/tsc": "cd __spec__ && tsc",
    "eslint": "eslint --fix ."
  },
  "author": {
    "email": "kirmas@phystech.edu",
    "name": "Andrii Kirmas",
    "url": "https://about.me/kirmas"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/askirmas/css.git"
  },
  "dependencies": {
    "@types/jest": "^26.0.8",
    "git-hooks-wrapper": "^0.7.1"
  },
  "devDependencies": {
    "@logux/eslint-config": "^33.0.0",
    "@types/globby": "^9.1.0",
    "eslint": "^6.5.1",
    "eslint-config-postcss": "^3.0.7",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-es5": "^1.4.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.19.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^12.1.0",
    "globby": "^11.0.1",
    "jest": "^24.9.0",
    "np": "^6.3.2",
    "postcss": "^7.0.18",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "postcss": ">=7"
  },
  "eslintConfig": {
    "extends": "eslint-config-postcss"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "bugs": {
    "url": "https://github.com/askirmas/css/labels/bug"
  },
  "homepage": "https://github.com/askirmas/css#readme",
