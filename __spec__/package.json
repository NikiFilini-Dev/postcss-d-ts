    "prebuild": "npm run reinstall"
  },
  "license": "MIT",
  "dependencies": {
    "@types/react": "^16.9.44",
    "postcss-plugin-d-ts": "file:..",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.26.10"
  }
}
