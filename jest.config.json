{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "modulePathIgnorePatterns": [
    "css\\.d\\.ts$"
  ],
  "rootDir": ".",
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/**/*.{ts,tsx,js,jsx}"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  }
}