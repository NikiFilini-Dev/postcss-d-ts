{
  "displayName": "func",
  "coverageDirectory": "coverage",
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/sandbox/",
    "/__sandbox__/",
    "-src"
  ],
  "collectCoverageFrom": [
    "src/**/*.{ts,tsx,js,jsx}"
  ],
  "preset": "ts-jest",
  "testEnvironment": "node",
  "collectCoverage": true,
  "coverageThreshold": {
    "global": {
      "statements": 100,
      "branches": 100,
      "functions": 100,
      "lines": 100
    }
  }
}
