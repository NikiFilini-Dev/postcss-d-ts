{
  "displayName": "func",
  "coverageDirectory": "coverage",
  "testRegex": ".*\\.(test|spec)\\.[jt]sx?$",
  "collectCoverageFrom": [
    "src/**/*.{ts,tsx,js,jsx}",
    "!src/coverage/**/*.*",
    "!**/*.(test|spec).{ts,tsx,js,jsx}"
  ],
  "preset": "ts-jest",
  "testEnvironment": "node",
  "collectCoverage": true,
  "coverageThreshold": {
    "global": {
      "statements": 100,
      "branches": 100,
      "functions": 100,
      "lines": 100
    }
  },
  "modulePathIgnorePatterns": [
    "__unit__/index.*\\.d\\.ts",
    ".*__sandbox__.*"
  ]
}