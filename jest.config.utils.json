{
  "displayName": "utils",
  "coverageDirectory": "coverage-utils",
  "testRegex": "src/.*\\.(test|spec)\\.[jt]sx?$",
  "preset": "ts-jest",
  "testEnvironment": "node",
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/**/*.{ts,tsx,js,jsx}",
    "!src/{index,7}.ts"
  ],
  "coverageThreshold": {
    "global": {
      "statements": 100,
      "branches": 100,
      "functions": 100,
      "lines": 100
    }
  }
}
