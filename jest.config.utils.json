{
  "displayName": "utils",
  "coverageDirectory": "coverage-utils",
  "testRegex": "src/.*\\.(test|spec)\\.[jt]sx?$",
  "collectCoverageFrom": [
    "src/**/*.{ts,tsx,js,jsx}",
    "!**/*.(test|spec).{ts,tsx,js,jsx}",
    "!src/coverage/**/*.*",
    "!src/index.ts"
  ],
  "preset": "ts-jest",
  "testEnvironment": "node",
  "collectCoverage": true,
  "coverageThreshold": {
    "global": {
      "statements": 100,
      "branches": 100,
      "functions": 100,
      "lines": 100
    }
  },
  "modulePathIgnorePatterns": [
    "__unit__/index.*\\.d\\.ts"
  ]
}