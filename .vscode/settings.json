{
  "json.schemas": [
    {
      "fileMatch": [
        "jest.config.json"
      ],
      "url": "https://askirmas.github.io/jest.schema.json"
    },
    {
      "fileMatch": [
        "postcss-d-ts.config.json"
      ],
      "url": "./src/schema.json"
    }
  ],
  "sonarlint.testFilePattern": "{**/*.test.*, **/*.spec.*}",
  "deepscan.enable": true
}
